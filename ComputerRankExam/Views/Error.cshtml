@model ComputerRankExam.Models.Error
@{
    ViewBag.Title = "发生错误";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form">
    <h4>@Html.DisplayFor(model => model.Title)</h4>
    <hr />
    <dl>
        <dt>@Html.DisplayFor(model => model.Title)</dt>
        <dd>@Html.Raw(Model.Details)</dd>
        <dd>@Html.Raw(Model.Cause)</dd>
        <dd>@Html.Raw(Model.Solution)</dd>
    </dl>
    <div class="clear"></div>
</div>
